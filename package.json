{
  "name": "shopping.products",
  "version": "1.0.0",
  "description": "Products microservice",
  "scripts": {
    "start": "./node_modules/.bin/sls offline start -r us-east-1 --noTimeout",
    "install:dynamodb": "./node_modules/.bin/sls dynamodb install",
    "start:dynamodb": "./node_modules/.bin/sls dynamodb start -p 8000  --migrate true",
    "test:integration": "env TEST_MODE=handler ./node_modules/.bin/mocha tests/integration_tests/test_cases --reporter spec --retries 3 --timeout 10000"
  },
  "dependencies": {
    "co": "^4.6.0",
    "serverless-dynamodb-client": "0.0.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.207.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "mocha": "^5.0.4",
    "serverless": "^1.26.1",
    "serverless-dynamodb-local": "^0.2.28",
    "serverless-iam-roles-per-function": "^0.1.5",
    "serverless-offline": "^3.18.0",
    "serverless-pseudo-parameters": "^1.4.2",
    "serverless-webpack": "^3.1.1",
    "webpack": "^3.3.0"
  },
  "author": "Petar Korudzhiev",
  "license": "ISC"
}
